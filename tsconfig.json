{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "out",
    "resolveJsonModule": true,
    "rootDir": "."
  },
  "include": [
    "./src",
    "index.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}