{
  "name": "markdoc-ls",
  "description": "Markdoc language server in node",
  "version": "0.7.1",
  "author": "Mohit Singh <mohit@mohitsingh.in>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohitsinghs/markdoc-ls"
  },
  "scripts": {
    "prebuild": "npx patch-package",
    "build": "node ./scripts/build.mjs",
    "docs": "node ./scripts/docs.mjs",
    "postdocs": "prettier --write \"./data/*.json\"",
    "test": "node -r esbuild-register src/**/*.spec.ts"
  },
  "bin": "./bin/markdoc-ls",
  "main": "./dist/index.js",
  "dependencies": {
    "esbuild-wasm": "^0.18.10"
  },
  "devDependencies": {
    "@markdoc/markdoc": "^0.3.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.43.0",
    "js-yaml": "^4.1.0",
    "lodash.merge": "^4.6.2",
    "markdown-it": "^13.0.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vscode-languageserver": "^8.1.0",
    "vscode-languageserver-textdocument": "^1.0.8",
    "vscode-uri": "^3.0.7"
  }
}
